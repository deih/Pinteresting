<div class="jumbotron">
<h1>Welcome to the copy of Pinterest!</h1>
	  <% if user_signed_in? %>
	  <div class="center">
         <%= link_to "Add Pin", new_pin_path, class: "btn btn-danger btn-lg" %>
         </div>
     <% else %>
     <div class="center">
       <p>
         <%= link_to "Log in", new_user_session_path, class: "btn btn-default btn-lg" %>
         <%= link_to "Sign up", new_user_registration_path, class: "btn btn-primary btn-lg" %>
       </p>
      </div>
  <% end %>  
</div>
